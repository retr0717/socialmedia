{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "types": ["@types/babel__core"],
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.js",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    ".expo/types/**/*.js",
    "expo-env.d.ts"
  ]
}
